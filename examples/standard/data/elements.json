    "actions": ["onvalue"]
  },

  "scene": {
    "properties": ["steps", "delay"],
    "events": ["start"]
  },

  "ota": {
    "sys": "true",
    "properties": ["port", "passwd"]
  },

  "ssdp": {
    "sys": "true",
    "properties": ["manufacturer", "manufacturerurl", "modelurl", "modelnumber"]
  },

  "rtcstate": {
    "sys": "true"
  },

